#lgbtn {
    position: relative;
    top: 0px;
    left: 10px;
    height: 20px;
    width: 48px;
    border: 1px dodgerblue solid;
    background-image:url(../images/lgbtn.gif);
    background-position: center;
    background-repeat: no-repeat;
}

#rgbtn {
    position: relative;
    top: 0px;
    left: 20px;
    height: 20px;
    width: 59px;
    border: 1px dodgerblue solid;
    background-image:url(../images/rgbtn.gif);
    background-position: center;
    background-repeat: no-repeat;
}

#fdbtn {
    position: relative;
    top: 0px;
    left: 30px;
    height: 20px;
    width: 59px;
    border: 1px dodgerblue solid;
    background-image:url(../images/fdbtn.gif);
    background-position: center;
    background-repeat: no-repeat;
}

#lgname, #lgpwd {
    border: 1px dodgerblue solid;
    width: 135px;
    height: 18px;
}

#lgchk {
    border: 1px dodgerblue solid;
    width: 135px;
    height: 16px;
}

#chkid {
    position: relative;
    width: 50px;
    top: 0px;
    left: 2px;
    height: 18px;
    border: 0px;
}

#changea {
    position: relative;
    top: 0px;
    left: 5px;
    cursor: hand;
}

#regimg {
    position: relative;
    top: 340px;
    left: 0px;
    height: 30px;
    width: 245px;
    background-image:url(../images/loading.gif);
    background-position: center;
    background-repeat: no-repeat;
}


