/* 找回密码页面  */
#fdbgdiv{
    margin:0 auto;
    width:296px;
    height:196px;
    border:2px #E8B38B solid;
    background-color:#FFFFFF;
}
#top{
    height:25px;
    border-bottom: 1px #E8B38B solid;
    /*background-image:url(../images/fdbg.gif);*/
    background-position:top;
    background-repeat:repeat-x;
    text-align:left;
    line-height:25px;
    text-indent:10px;
    font-weight:bolder;
}
#foundname,#fdquestion,#fdanswer{
    width: 100px;
    height:15px;
    border:1px #000000 solid;
}
#foundnamediv{
    position: relative;
