/* 找回密码页面  */
#fdbgdiv {
    margin: 0 auto;
    width: 296px;
    height: 196px;
    border: 2px #E8B38B solid;
    background-color: #FFFFFF;
}

#top {
    height: 25px;
    border-bottom: 1px #E8B38B solid;
    /*background-image:url(../images/fdbg.gif);*/
    background-position: top;
    background-repeat: repeat-x;
    text-align: left;
    line-height: 25px;
    text-indent: 10px;
    font-weight: bolder;
}

#foundname, #fdquestion, #fdanswer {
    width: 100px;
    height: 15px;
    border: 1px #000000 solid;
}

#foundnamediv {
    position: relative;
    top: 20px;
    left: 50px;
    width: 170px;
}

#step1 {
    position: relative;
    top: 15px;
    margin: 0 auto;
    width: 60px;
    height: 18px;
    /*background-image:url(../images/finish.gif);*/
    background-repeat: repeat-x;
    background-position: center;
    border: 1px dodgerblue solid;
}

#bottom {
    position: absolute;
    /*background-image: url(../images/bottom.gif);*/
    background-repeat: no-repeat;
    background-position: right;
    bottom: 15px;
    right: 15px;
    width: 100%;
    height: 20px;
}

